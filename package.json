  "homepage": "https://github.com/NestboxAI/nestbox-ai-cli-tools#readme",
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^6.0.3",
    "@types/form-data": "^2.5.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
