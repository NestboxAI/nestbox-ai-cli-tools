    "coverage": "vitest run --coverage",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NestboxAI/nestbox-ai-cli-tools.git"
  },
  "keywords": [
    "cli"
  ],
