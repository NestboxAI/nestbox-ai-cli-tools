{
  "name": "@nestbox-ai/cli",
  "version": "1.0.0",
  "description": "The cli tools that helps developers to build agents",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
